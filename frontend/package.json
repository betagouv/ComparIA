{
  "name": "comparia-sveltekit",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:e2e": "playwright test",
    "test": "npm run test:e2e && npm run test:unit -- --run",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.1",
    "@eslint/js": "^9.39.2",
    "@iconify-json/ri": "^1.2.7",
    "@inlang/paraglide-js": "^2.9.1",
    "@lobehub/icons-static-svg": "^1.78.0",
    "@playwright/test": "^1.58.0",
    "@sveltejs/adapter-node": "^5.5.2",
    "@sveltejs/kit": "^2.50.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.1",
    "@types/prismjs": "^1.26.5",
    "@types/sanitize-html": "^2.16.0",
    "@unocss/extractor-svelte": "^66.6.0",
    "@unocss/preset-icons": "^66.6.0",
    "@unocss/preset-wind4": "^66.6.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.14.0",
    "globals": "^17.1.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "svelte": "^5.48.0",
    "svelte-check": "^4.3.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "unocss": "^66.6.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.14.0",
    "amuchina": "^1.0.12",
    "d3": "^7.9.0",
    "github-slugger": "^2.0.0",
    "marked-gfm-heading-id": "^4.1.3",
    "marked-highlight": "^2.2.3",
    "mermaid": "^11.12.2",
    "prismjs": "^1.30.0",
    "sanitize-html": "^2.17.0"
  }
}
