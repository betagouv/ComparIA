{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":disableDependencyDashboard"
  ],
  "packageRules": [
      {
          "matchDatasources": ["docker"],
          "matchDepNames": ["python"],
          "enabled": false
      }
  ],
  "poetry": {
    "fileMatch": [
      "(^|/)pyproject\\.toml$"
    ],
    "rangeStrategy": "in-range-only"
  }
}
